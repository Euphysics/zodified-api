{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "!.next/standalone/node_modules/**",
        "dist/**",
        "!.turbo/**"
      ]
    },
    "lint": {}
  },
  "concurrency": "2"
}
